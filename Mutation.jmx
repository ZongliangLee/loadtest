            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor" enabled="true">
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">def labels = [&quot;Google&quot;, &quot;Yahoo&quot;, &quot;Facebook&quot;]
def matrix = [
	[[0,0],[0.5,1000],[0.2,1000]],
	[[0,0],[0.3,1200],[0.3,1200]],
	[[0,0],[0.1,700],[0.4,700]],
	[[0,0],[0.1,1100],[0.7,1100]],
]

def state = vars.get(&quot;state&quot;)
def nextState = null
def holdingLambda = 0
def holdingScale = Double.parseDouble(props.get(&quot;holdingScale&quot;) ?: &quot;1&quot;)
def px = Math.random()
def py = 0
def v = matrix[labels.indexOf(state)]

for (x=0;x&lt;v.size();x++) {
	def e = v[x]
	py += e[0]
	if (px &lt; py) {
		nextState = labels[x]
		holdingLambda = e[1]
		break
	}
	
}
if (!nextState) {
	vars.put(&quot;active&quot;, &quot;false&quot;)
} else {
	def finalHolding = holdingLambda = holdingScale
	vars.put(&quot;state&quot;,nextState)
	vars.put(&quot;holdingLambda&quot;,finalHolding.toString())
}
</stringProp>
              <stringProp name="scriptLanguage">groovy</stringProp>
